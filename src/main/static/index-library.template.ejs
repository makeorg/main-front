    <script>
      var targetHash = "#/consultation/comment-lutter-contre-les-violences-faites-aux-femmes"
      var targetUrl = "https://stopvff.make.org/" + targetHash

      if(/http(s)?:\/\/(www\.)?stopvff\.+/.test(document.location)) {
        if (!/\/#\/account-activation\/.+/.test(document.location.hash) &&
            document.location.hash != targetHash
         ) {
            window.location = targetUrl
        }
      }
    </script>
