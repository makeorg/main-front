<!DOCTYPE html>
<html>
<head>
    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
    <link rel="manifest" href="/manifest.json">
    <link rel="mask-icon" href="/safari-pinned-tab.svg" color="#5bbad5">
    <meta name="theme-color" content="#ffffff">
    <meta charset="utf-8">
    <title><%= htmlWebpackPlugin.options.title %></title>
    <meta content="<%= htmlWebpackPlugin.options.title %>" property="og:title">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1" name="viewport">
    <link href="https://fonts.googleapis.com/css?family=Playfair+Display:400i" rel="stylesheet">
    <link rel="stylesheet" href="./styles/main.css" type="text/css" media="all">
</head>
<body>
<script>
    // simulate emitting events to facebook pixel
    window.fbq = function(eventType, eventName, eventParameters){
        console.log("FBEvent type=", eventType, "name=", eventName, "params=", eventParameters)
    }
</script>
<div id="make-app">
    <script>
        fbq('display-loader', {})
    </script>
    <style>
      html,body,#make-app{height:100%;margin:0}
      .loader__wrapper{display:table;width:100%;height:100%;background:rgba(0,0,0,0.05)}
      .loader__inner-wrapper{display:table-cell;vertical-align:middle;padding:1em;text-align:center}
      @-webkit-keyframes FLY {
        0%{-webkit-transform:translate(2px,2px);-ms-transform:translate(2px,2px);transform:translate(2px,2px)}
        20%{-webkit-transform:translate(2px,0px);-ms-transform:translate(2px,0px);transform:translate(2px,0px)}
        40%{-webkit-transform:translate(2px,1px);-ms-transform:translate(2px,1px);transform:translate(2px,1px)}
        60%{-webkit-transform:translate(1px,1px);-ms-transform:translate(1px,1px);transform:translate(1px,1px)}
        100%{-webkit-transform:translate(1px,0px);-ms-transform:translate(1px,1px);transform:translate(1px,0px)}
      }
      .loader__ill{max-width:100%;-webkit-animation:FLY 0.3s infinite;-moz-animation:FLY 0.3s infinite;-o-animation:FLY 0.3s infinite;animation:FLY 0.3s infinite;}
      .loader__message{margin:1em 0;font-family:CircularStdBook,Arial,sans-serif;font-size:0.9375em;line-height:1.3333333333333333;color:rgba(0,0,0,0.3)}
      .loader__message em{font-family:CircularStdBold,Arial,sans-serif;font-weight:bold;font-size:1.2em;line-height:1.27777777778;color:#ed1844}
      @media (max-width: 31.1875em){.loader__ill{width:5em}}
      @media (min-width: 31.25em){.loader__message{font-size:1.5em;}.loader__message em{font-size:1.16666666667em;line-height:1.3333333333333333;}}
    </style>
    <div class="loader__wrapper">
      <div class="loader__inner-wrapper">
        <img src="/loader.gif" srcset="/loader@2x.gif 2x" alt="load">
      </div>
    </div>
</div>
<script type="application/javascript" src="make-front-fastopt-library.js"></script>
<script language="javascript">
    var configuration = {
        "apiUrl": "<%= htmlWebpackPlugin.options.apiUrl %>",
        "googleAppId": "<%= htmlWebpackPlugin.options.googleAppId %>",
        "facebookAppId": "<%= htmlWebpackPlugin.options.facebookAppId %>",
        "detectedCountry": "<%= htmlWebpackPlugin.options.detectedCountry %>",
        "forcedCountry": "<%= htmlWebpackPlugin.options.forcedCountry %>"
    };
    var exports = window;
    exports.require = window["makeApp"].require;
</script>

<script type="application/javascript" src="make-front-fastopt.js"></script>
</body>
</html>